pipeline {
  agent any
  stages {
    stage('plan') {
      steps {
        echo 'to plan app'
      }
    }

    stage('code') {
      steps {
        echo 'full code for app'
      }
    }

    stage('Build') {
      steps {
        echo 'To build the pipeline'
      }
    }

    stage('Test') {
      steps {
        echo 'To test the pipeline'
      }
    }

    stage('Release') {
      steps {
        echo 'To release the sprint'
      }
    }

    stage('Deploy') {
      steps {
        echo 'To deploy the application'
      }
    }

    stage('Operate') {
      steps {
        echo 'To operate the application'
      }
    }

  }
}